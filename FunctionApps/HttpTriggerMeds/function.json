{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "req",
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "methods": [
        "get",
        "post"
      ],
      "route": "meds"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "out",
      "path": "httptriggermeds/{rand-guid}",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
